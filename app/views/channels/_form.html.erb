<%= form_for(@channel) do |f| %>
	<% unless @channel.errors.empty? %>
		<div id="error_explanation">
			<h2> Исправьте ошибку </h2>
			<ul>
			  <% @channel.errors.full_messages.each do |msg| %>
			    <li><%= msg %></li>
			</ul>
		  <% end %>
		</div>
	<% end %>
	<div class="form-group">
		<%= f.label :name %>
  	<%= f.text_field :name %>
  </div>

  <div class="form-group">
    <%= f.submit "Создать", class: 'btn btn-primary' %>
  </div>
<% end %>
